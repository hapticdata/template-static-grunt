{
	"name": "static-grunt",
	"version": "0.2.1",
	"author": "Kyle Phillips",
	"description": "A template for building static-generated sites with the use of [grunt](http://gruntjs.com), [compass](http://compass-style.org) and [jade](http://jade-lang.com)",
    "repository": {
        "type": "git",
        "url": "https://github.com/hapticdata/template-static-grunt.git"
    },
	"fetch": {
		"animitter": {
			"file": "https://raw.github.com/hapticdata/animitter/master/animitter.js",
			"target": "public/javascripts/vendor/animitter.js"
		},
		"async": {
			"file": "https://raw.github.com/caolan/async/master/lib/async.js",
			"target": "public/javascripts/vendor/async.js"
		},
        "jade-runtime": {
            "file": "https://raw.github.com/visionmedia/jade/master/runtime.js",
            "target": "public/javascripts/vendor/jade.js"
        },
		"jquery": {
			"file": "http://code.jquery.com/jquery.js",
			"target": "public/javascripts/vendor/jquery.js"
		},
		"datgui": {
			"clone": "https://code.google.com/p/dat-gui/",
			"target": {
				"src/dat": "public/javascripts/vendor/dat"
			}
		},
		"require": {
			"file": "https://raw.github.com/jrburke/requirejs/master/require.js",
			"target": "public/javascripts/vendor/require.js"
		},
		"domReady": {
			"file": "https://raw.github.com/requirejs/domReady/master/domReady.js",
			"target": "public/javascripts/vendor/domReady.js"
		},
		"text": {
			"file": "https://raw.github.com/requirejs/text/master/text.js",
			"target": "public/javascripts/vendor/text.js"
		},
		"toxi": {
			"clone": "https://github.com/hapticdata/toxiclibsjs.git",
			"target": {
				"lib/toxi": "public/javascripts/vendor/toxi"
			}
		},
		"Backbone-AMD": {
            "file": "https://raw.github.com/amdjs/backbone/master/backbone.js", 
			"target": "public/javascripts/vendor/backbone.js"
		},
		"underscore-AMD": {
            "file": "https://raw.github.com/amdjs/underscore/master/underscore.js",
			"target": "public/javascripts/vendor/underscore.js"
		}
	}
}
